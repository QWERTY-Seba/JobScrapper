{
    "name": "JobScrapper",
    "version": "1",
    "description": "",
    "manifest_version": 3,
    "author": "Seba",
	  "content_scripts": [
        {
          "matches": ["https://www.linkedin.com/jobs/search/*"],
          "js": ["content_script.js"]
        }
      ],
	"permissions":["background","debugger","storage","unlimitedStorage","tabs","activeTab","scripting"],
    "action":{
        "default_popup": "action.html",
        "default_title": "_"
    },
	"background": {
		"service_worker": "background.js",
    "type" : "module"
	}
}